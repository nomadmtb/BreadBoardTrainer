<Page x:Class="BreadBoardTrainer.LaunchPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BreadBoardTrainer"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="LaunchPage" Width="1920" Height="1080">
    <Canvas Height="1080" Width="1920" HorizontalAlignment="Left" VerticalAlignment="Top">
        <Image x:Name="image" Height="200" Canvas.Left="0" Canvas.Top="0" Width="1740" Source="/BreadBoardTrainer;component/Images/hub_logo.png" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="90,90,0,0">
            <Image.OpacityMask>
                <ImageBrush ImageSource="/BreadBoardTrainer;component/Images/hub_logo.png"/>
            </Image.OpacityMask>
        </Image>
        <Button x:Name="pushButton" Content="PUSH ME" Canvas.Left="0" Canvas.Top="0" Width="76" HorizontalAlignment="Center" VerticalAlignment="Center" Height="21"/>
    </Canvas>
</Page>
